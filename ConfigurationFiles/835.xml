<medi:edimap xmlns:medi="http://www.milyn.org/schema/edi-message-mapping-1.0.xsd">

  <medi:description name="HP 835" version="1.0" />

  <medi:delimiters segment="&#126;" field="*" component=">" sub-component="" />

  <medi:segments xmltag="Health Care Claim Payment Advice">

    <medi:segment segcode="ISA" xmltag="Interchange Control Header">
      <medi:field xmltag="ISA01 I01 Authorization Information Qualifier" />
      <medi:field xmltag="ISA02 I02 Authorization Information" />
      <medi:field xmltag="ISA03 I03 Security Information Qualifier" />
      <medi:field xmltag="ISA04 I04 Security Information" />
      <medi:field xmltag="ISA05 I05 Interchange ID Qualifier - Sender" />
      <medi:field xmltag="ISA06 I06 Interchange Sender ID" />
      <medi:field xmltag="ISA07 I05 Interchange ID Qualifier - Receiver" />
      <medi:field xmltag="ISA08 I07 Interchange Receiver ID" />
      <medi:field xmltag="ISA09 I08 Interchange Date" />
      <medi:field xmltag="ISA10 I09 Interchange Time" />
      <medi:field xmltag="ISA11 I10 Interchange Control Standards Identifier" />
      <medi:field xmltag="ISA12 I11 Interchange Control Version Number" />
      <medi:field xmltag="ISA13 I12 Interchange Control Number" />
      <medi:field xmltag="ISA14 I13 Acknowledgement Requested" />
      <medi:field xmltag="ISA15 I14 Test Indicator" />
      <medi:field xmltag="ISA16 I15 Subelement Separator"/>
    </medi:segment>

   <medi:segment segcode="GS" xmltag="Functional Group Header">
      <medi:field xmltag="GS01 G01 Functional Identifier Code" />
      <medi:field xmltag="GS02 G02 Application Sender's Code" />
      <medi:field xmltag="GS03 G03 Application Receiver's Code" />
      <medi:field xmltag="GS04 G04 Group Date" />
      <medi:field xmltag="GS05 G05 Group Time" />
      <medi:field xmltag="GS06 G06 Group Control Number" />
      <medi:field xmltag="GS07 G07 Responsible Agency Code" />
      <medi:field xmltag="GS08 G08 Version/Release/Industry ID Code" />
    </medi:segment>

    <medi:segment segcode="ST" xmltag="Transaction Set Header" >
      <medi:field xmltag="ST01 143 Transaction Set Identifier Code" />
      <medi:field xmltag="ST02 329 Transaction Set Control Number" />

      <medi:segment segcode="BPR" xmltag="Financial Information">
        <medi:field xmltag="BPR01 Transaction handling code" />
        <medi:field xmltag="BPR02 Total actual provider payment amount" />
        <medi:field xmltag="BPR03 Credit or debit flag code" />
		<medi:field xmltag="BPR04 Payment method code" />
		<medi:field xmltag="BPR05 Payment format code" />
		<medi:field xmltag="BPR06 Depository financial institution (DFI) identification number qualifier" />
		<medi:field xmltag="BPR07 Sender DFI identifier" />
		<medi:field xmltag="BPR08 Account number qualifier" />
		<medi:field xmltag="BPR09 Sender Bank Account Number" />
		<medi:field xmltag="BPR10 Payer identifier" />
		<medi:field xmltag="BPR11 Originating company supplemental code" />
		<medi:field xmltag="BPR12 Depository financial institution (DFI) identification number qualifier" />
		<medi:field xmltag="BPR13 Receiver or provider bank ID number" />
		<medi:field xmltag="BPR14 Account number qualifier" />
		<medi:field xmltag="BPR15 Receiver or provider account number" />
		<medi:field xmltag="BPR16 Check issue or EFT effective date in CCYYMMDD format" />
      </medi:segment>
	  
      <medi:segment segcode="TRN" xmltag="Reassociation Trace Number">
        <medi:field xmltag="TRN01 Trace type code" />
        <medi:field xmltag="TRN02 Check or EFT trace number" />
        <medi:field xmltag="TRN03 Payer identifier" />
		<medi:field xmltag="TRN04 Originating company supplemental code" />
      </medi:segment>
	  
      <medi:segment segcode="REF" xmltag="Receiver Identification">
        <medi:field xmltag="REF01 Receiver Identification Number" />
        <medi:field xmltag="REF02 Receiver REFERENCE IDENTIFICATION" />
      </medi:segment>
	  
      <medi:segment segcode="DTM" xmltag="Production Date">
        <medi:field xmltag="DTM01 Date Qualifier" />
        <medi:field xmltag="DTM02 Date" />
      </medi:segment>
	  
      <medi:segment segcode="N1" xmltag="Payer Identification">
        <medi:field xmltag="N101 Payer Identifier Code" />
        <medi:field xmltag="N102 Payer NAME" />
		<medi:field xmltag="N103 Identification Code Qualifier" />
		<medi:field xmltag="N104 Payer Identification Code" />
      </medi:segment>
	  
	  <medi:segment segcode="N3" xmltag="Payer Address">
        <medi:field xmltag="N301 Payer Address Line" />
        <medi:field xmltag="N302 Payer Address Line" />
      </medi:segment>

	  <medi:segment segcode="N4" xmltag="Payer City, State, Zip Code">
        <medi:field xmltag="N401 Payer City Name" />
        <medi:field xmltag="N402 Payer State Code" />
		<medi:field xmltag="N403 Payer Postal Zone or Zip Code" />
      </medi:segment>

	  <medi:segment segcode="REF" xmltag="Payee Additional Identification">
        <medi:field xmltag="REF01 Additional Payee Identification Qualifier" />
        <medi:field xmltag="REF02 Reference Identification Code" />
      </medi:segment>

	  <medi:segment segcode="LX" xmltag="Header Number">
        <medi:field xmltag="LX01 Claim Sequence Number" />
      </medi:segment>

	  
	  <medi:segment segcode="CLP" xmltag="Claim payment Information" minOccurs="0" maxOccurs="-1">	  
        <medi:field xmltag="CLP01 Patient Control Number" />
        <medi:field xmltag="CLP02 Claim Status Code" />
        <medi:field xmltag="CLP03 Total Claim Charge Amount" />
        <medi:field xmltag="CLP04 Total Claim Payment Amount" />
        <medi:field xmltag="CLP05 Patient Responsibility Amount" />
        <medi:field xmltag="CLP06 Claim Filing Indicator Code" />
        <medi:field xmltag="CLP07 Payer Claim Control Number" />
        <medi:field xmltag="CLP08 Facility Type Code" />
        <medi:field xmltag="CLP09 Claim Frequency Code" />
        <medi:field xmltag="CLP10 Patient Status Code" />
        <medi:field xmltag="CLP11 Diagnosis Related Group (DRG) Code" />
        <medi:field xmltag="CLP12 DRG Weight" />
        <medi:field xmltag="CLP13 PERCENT - Discharge Fraction" />

		
		<medi:segment segcode="NM1" xmltag="Patient Name">
		  <medi:field xmltag="NM101 Patient Identifier Code" />
		  <medi:field xmltag="NM102 Entity Type Qualifier" />
		  <medi:field xmltag="NM103 Patient Last Name" />
		  <medi:field xmltag="NM104 Patient First Name" />
		  <medi:field xmltag="NM105 Patient Middle Initial" />
		  <medi:field xmltag="NM106 Name Prefix" />
		  <medi:field xmltag="NM107 Patient Name Suffix" />
		  <medi:field xmltag="NM108 Identification Code Qualifier" />
		  <medi:field xmltag="NM109 Patient Member Number" />
		</medi:segment>

		<medi:segment segcode="SVC" xmltag="Service Payment Information" >
		  <medi:field xmltag="SVC01 Service Type Code" />
		  <medi:field xmltag="SVC02 Monetary Amount" />
		  <medi:field xmltag="SVC03 Monetary Amount" />
		  <medi:field xmltag="SVC04 NUBC Revenue Code" />
		  <medi:field xmltag="SVC05 Units of Service Paid Count" />
		</medi:segment>		
		
		<medi:segment segcode="DTM" xmltag="Coverage Expiration Date" >
          <medi:field xmltag="DTM01 Date Time Qualifier" />
		  <medi:field xmltag="DTM02 Service Date" />
        </medi:segment>
		
		<medi:segment segcode="CAS" xmltag="Claim Adjustment" >
		  <medi:field xmltag="CAS01 Claim Adjustment Group Code" />
		  <medi:field xmltag="CAS02 Adjustment Reason Code" />
		  <medi:field xmltag="CAS03 Adjustment Amount" />
		  <medi:field xmltag="CAS04 QUANTITY" />
		</medi:segment>

		<medi:segment segcode="REF" xmltag="Line Item Control Number">
          <medi:field xmltag="REF01 Reference Identification Qualifier" />
          <medi:field xmltag="REF02 Reference Identification" />
		</medi:segment>

		
		<medi:segment segcode="AMT" xmltag="Service Supplemental Amount">
          <medi:field xmltag="AMT01 Amount Qualifier Code" />
		  <medi:field xmltag="AMT02 Service Line Allowed Amount" />
        </medi:segment>

	  </medi:segment>

      <medi:segment segcode="SE" xmltag="Transaction Set Trailer">
        <medi:field xmltag="SE01 96 Number of Included Segments" />
        <medi:field xmltag="SE02 329 Transaction Set Control Number" />
      </medi:segment>

    </medi:segment>

    <medi:segment segcode="GE" xmltag="Functional Group Trailer">
      <medi:field xmltag="GE01 97 Number of Transaction Sets Included" />
      <medi:field xmltag="GE02 28 Group Control Number" />
    </medi:segment>

    <medi:segment segcode="IEA" xmltag="Interchange Trailer">
      <medi:field xmltag="IEA01 I16 Number of Included Functional Groups" />
      <medi:field xmltag="IEA02 I12 Interchange Control Number" />
    </medi:segment>

  </medi:segments>

</medi:edimap>
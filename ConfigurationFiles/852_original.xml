<medi:edimap xmlns:medi="http://www.milyn.org/schema/edi-message-mapping-1.0.xsd">

  <medi:description name="PD 852" version="1.0" />

  <medi:delimiters segment="&#10;" field="*" component=">" sub-component="" />

  <medi:segments xmltag="Product Activity Data">

    <medi:segment segcode="ISA" xmltag="Interchange Control Header">
      <medi:field xmltag="ISA01 I01 Authorization Information Qualifier" />
      <medi:field xmltag="ISA02 I02 Authorization Information" />
      <medi:field xmltag="ISA03 I03 Security Information Qualifier" />
      <medi:field xmltag="ISA04 I04 Security Information" />
      <medi:field xmltag="ISA05 I05 Interchange ID Qualifier - Sender" />
      <medi:field xmltag="ISA06 I06 Interchange Sender ID" />
      <medi:field xmltag="ISA07 I05 Interchange ID Qualifier - Receiver" />
      <medi:field xmltag="ISA08 I07 Interchange Receiver ID" />
      <medi:field xmltag="ISA09 I08 Interchange Date" />
      <medi:field xmltag="ISA10 I09 Interchange Time" />
      <medi:field xmltag="ISA11 I10 Interchange Control Standards Identifier" />
      <medi:field xmltag="ISA12 I11 Interchange Control Version Number" />
      <medi:field xmltag="ISA13 I12 Interchange Control Number" />
      <medi:field xmltag="ISA14 I13 Acknowledgement Requested" />
      <medi:field xmltag="ISA15 I14 Test Indicator" />
      <medi:field xmltag="ISA16 I15 Subelement Separator"/>
    </medi:segment>

    <medi:segment segcode="GS" xmltag="Functional Group Header">
      <medi:field xmltag="GS01 G01 Functional Identifier Code" />
      <medi:field xmltag="GS02 G02 Application Sender's Code" />
      <medi:field xmltag="GS03 G03 Application Receiver's Code" />
      <medi:field xmltag="GS04 G04 Group Date" />
      <medi:field xmltag="GS05 G05 Group Time" />
      <medi:field xmltag="GS06 G06 Group Control Number" />
      <medi:field xmltag="GS07 G07 Responsible Agency Code" />
      <medi:field xmltag="GS08 G08 Version/Release/Industry ID Code" />
    </medi:segment>

    <medi:segment segcode="ST" xmltag="Transaction Set Header" maxOccurs="-1">
      <medi:field xmltag="ST01 143 Transaction Set Identifier Code" />
      <medi:field xmltag="ST02 329 Transaction Set Control Number" />

      <medi:segment segcode="XQ" xmltag="Reporting Date/Action">
        <medi:field xmltag="XQ01 305 Transaction Handling Code" />
        <medi:field xmltag="XQ02 373 Period Week Start Date" />
        <medi:field xmltag="XQ03 373 Period Week Stop Date" />
      </medi:segment>

      <medi:segment segcode="XPO" xmltag="XPO">
        <medi:field xmltag="XPO1" />
        <medi:field xmltag="XPO2" />
      </medi:segment>

      <medi:segment segcode="N9\*BT" xmltag="BT Reference Number">
        <medi:field xmltag="N901 128 BT Reference Number Qualifier" />
        <medi:field xmltag="N902 127 BT Reference Number" />
      </medi:segment>

      <medi:segment segcode="N9\*DP" xmltag="DP Reference Number">
        <medi:field xmltag="N901 128 DP Reference Number Qualifier" />
        <medi:field xmltag="N902 127 DP Reference Number" />
      </medi:segment>

      <medi:segment segcode="LIN" xmltag="Item Identification" minOccurs="0" maxOccurs="-1" >
        <medi:field xmltag="LIN01 350 Assigned Identification" />
        <medi:field xmltag="LIN02 235 Product/Service ID qualifier" />
        <medi:field xmltag="LIN03 235 Product/Service ID" />
        <medi:field xmltag="LIN04 Reference Number Qualifier" />
        <medi:field xmltag="LIN05 Reference Number" />
        <medi:field xmltag="LIN06 235 Product/Service ID qualifier" />
        <medi:field xmltag="LIN07 235 Product/Service ID" />
        <medi:field xmltag="LIN08 Reference Number Qualifier" />
        <medi:field xmltag="LIN09 Reference Number" />

        <medi:segment segcode="ZA" xmltag="Product Activity Reporting" minOccurs="0" >
          <medi:field xmltag="ZA01 859 Activity Code" />
        </medi:segment>

        <medi:segment segcode="CTP" xmltag="CTP" >
          <medi:field xmltag="CTP01" />
          <medi:field xmltag="CTP02 Reference Number Qualifier" />
          <medi:field xmltag="CTP03 Reference Number" />
        </medi:segment>

        <medi:segment segcode="SDQ" xmltag="SDQ" truncatable="true" >
          <medi:field xmltag="SDQ01 355 Unit or Basis of Measurement Code" />
          <medi:field xmltag="SDQ02 66 Identification Code Qualifier" />
          <medi:field xmltag="SDQ03 Store Location Code" />
          <medi:field xmltag="SDQ04 Quantity" />
          <medi:field xmltag="SDQ05 Store Location Code" required="false" />
          <medi:field xmltag="SDQ06 Quantity" required="false" />
          <medi:field xmltag="SDQ07 Store Location Code" required="false" />
          <medi:field xmltag="SDQ08 Quantity" required="false" />
          <medi:field xmltag="SDQ09 Store Location Code" required="false" />
          <medi:field xmltag="SDQ10 Quantity" required="false" />
          <medi:field xmltag="SDQ11 Store Location Code" required="false" />
          <medi:field xmltag="SDQ12 Quantity" required="false" />
          <medi:field xmltag="SDQ13 Store Location Code" required="false" />
          <medi:field xmltag="SDQ14 Quantity" required="false" />
          <medi:field xmltag="SDQ15 Store Location Code" required="false" />
          <medi:field xmltag="SDQ16 Quantity" required="false" />
          <medi:field xmltag="SDQ17 Store Location Code" required="false" />
          <medi:field xmltag="SDQ18 Quantity" required="false" />
          <medi:field xmltag="SDQ19 Store Location Code" required="false" />
          <medi:field xmltag="SDQ20 Quantity" required="false" />
          <medi:field xmltag="SDQ21 Store Location Code" required="false" />
          <medi:field xmltag="SDQ22 Quantity" required="false" />
        </medi:segment>

      </medi:segment>

      <medi:segment segcode="CTT" xmltag="Transaction Totals">
        <medi:field xmltag="CTT01 354 Number of Line items" />
        <medi:field xmltag="CTT02 347 Hash Total" />
      </medi:segment>

      <medi:segment segcode="SE" xmltag="Transaction Set Trailer">
        <medi:field xmltag="SE01 96 Number of Included Segments" />
        <medi:field xmltag="SE02 329 Transaction Set Control Number" />
      </medi:segment>

    </medi:segment>

    <medi:segment segcode="GE" xmltag="Functional Group Trailer">
      <medi:field xmltag="GE01 97 Number of Transaction Sets Included" />
      <medi:field xmltag="GE02 28 Group Control Number" />
    </medi:segment>

    <medi:segment segcode="IEA" xmltag="Interchange Trailer">
      <medi:field xmltag="IEA01 I16 Number of Included Functional Groups" />
      <medi:field xmltag="IEA02 I12 Interchange Control Number" />
    </medi:segment>

  </medi:segments>

</medi:edimap>